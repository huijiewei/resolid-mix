{
  "version": 3,
  "routes": [
    {
      "src": "^/(favicon\\.ico|favicon\\.svg|apple-touch-icon\\.png|manifest\\.webmanifest)$",
      "headers": {
        "cache-control": "public, max-age=3600"
      },
      "continue": true
    },
    {
      "src": "^/icons/(.*)$",
      "headers": {
        "cache-control": "public, max-age=3600"
      },
      "continue": true
    },
    {
      "src": "^/assets/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "_serverless"
    }
  ]
}
