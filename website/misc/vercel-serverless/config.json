{
  "version": 3,
  "routes": [
    {
      "src": "^/(assets)/(.*)$",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "src": "^/(icons)/(.*)$",
      "headers": {
        "cache-control": "public, max-age=3600"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": ".*",
      "dest": "_serverless"
    }
  ]
}
